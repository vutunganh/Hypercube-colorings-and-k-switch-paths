CXX = g++
CXXDEBUG = -g -DDEBUG
CXXWARNS = -Wall -Wpedantic -Wextra -Werror
CXXFLAGS = -std=c++17 -O2 $(CXXWARNS) $(CXXDEBUG)
BINFILES = prog.out
OBJFILES = prog.o generators.o hypercube.o

all: $(BINFILES)

%.out: %.o $(OBJFILES)
	$(CXX) $(CXXFLAGS) $^ -o $@

.PHONY: clean clean-all

clean:
	rm -f $(OBJFILES)

clean-all: clean
	rm -f $(BINFILES)

# AUTOGENERATED BY `g++ -MM`, DO NOT EDIT!
generators.o: generators.cpp generators.hpp hypercube.hpp
hypercube.o: hypercube.cpp hypercube.hpp
prog.o: prog.cpp hypercube.hpp generators.hpp
